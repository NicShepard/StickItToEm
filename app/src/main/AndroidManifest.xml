<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.neu.madcourseworkupteam.stickittoem">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.StickItToEm">
        <activity
            android:name=".ReceivedActivity"
            android:label="@string/title_activity_received"
            android:theme="@style/Theme.StickItToEm.NoActionBar"></activity>
<!--        <activity-->
<!--            android:name=".SentHistoryActivity"-->
<!--            android:label="@string/title_activity_sent_history"-->
<!--            android:theme="@style/Theme.StickItToEm.NoActionBar" />-->
        <activity android:name=".RealTimeDatabaseActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service
            android:name=".MessagingService"
            android:exported="false">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
    </application>

</manifest>